{
    "name": "feed2toot",
    "initial_date": "2022-11-14",
    "github_url": "https://github.com/linuxserver/docker-feed2toot",
    "project_url": "https://gitlab.com/chaica/feed2toot",
    "project_logo": "",
    "description": "feed2toot automatically parses rss feeds, identifies new posts and posts them on the Mastodon social network.",
    "version": "0.17-ls108",
    "version_timestamp": "2025-03-28 23:37:30+00:00",
    "category": "RSS,Social",
    "stable": true,
    "deprecated": false,
    "stars": 4,
    "monthly_pulls": 132,
    "tags": [
        {
            "tag": "latest",
            "desc": "Stable releases"
        }
    ],
    "architectures": [
        {
            "arch": "x86_64",
            "tag": "amd64-latest"
        },
        {
            "arch": "arm64",
            "tag": "arm64v8-latest"
        }
    ],
    "changelog": [
        {
            "date": "2023-12-23",
            "desc": "Rebase to Alpine 3.19."
        },
        {
            "date": "2023-05-25",
            "desc": "Rebase to Alpine 3.18, deprecate armhf."
        },
        {
            "date": "2023-03-02",
            "desc": "Allow user customizable cron."
        }
    ],
    "config": {
        "application_setup": "https://github.com/linuxserver/docker-feed2toot?tab=readme-ov-file#application-setup",
        "env_vars": [
            {
                "name": "PUID",
                "value": "1000",
                "desc": "User ID",
                "optional": false
            },
            {
                "name": "PGID",
                "value": "1000",
                "desc": "Group ID",
                "optional": false
            },
            {
                "name": "TZ",
                "value": "Etc/UTC",
                "desc": "Timezone",
                "optional": false
            },
            {
                "name": "TZ",
                "value": "Europe/London",
                "desc": "Specify a timezone to use EG Europe/London.",
                "optional": false
            },
            {
                "name": "FEED_LIMIT",
                "value": "5",
                "desc": "Limit number of RSS entries published at each execution.",
                "optional": true
            }
        ],
        "volumes": [
            {
                "path": "/config",
                "host_path": "/path/to/feed2toot/config",
                "desc": "Persistent config files",
                "optional": false
            }
        ]
    }
}
