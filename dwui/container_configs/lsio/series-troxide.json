    "config": {
        "application_setup": "https://github.com/linuxserver/docker-series-troxide?tab=readme-ov-file#application-setup",
        "env_vars": [
            {
                "name": "PUID",
                "value": "1000",
                "desc": "User ID",
                "optional": false
            },
            {
                "name": "PGID",
                "value": "1000",
                "desc": "Group ID",
                "optional": false
            },
            {
                "name": "TZ",
                "value": "Etc/UTC",
                "desc": "Timezone",
                "optional": false
            },
            {
                "name": "TZ",
                "value": "Europe/London",
                "desc": "Specify a timezone to use EG Europe/London.",
                "optional": false
            }
        ],
        "volumes": [
            {
                "path": "/config",
                "host_path": "/path/to/config",
                "desc": "Users home directory in the container, stores local files and settings",
                "optional": false
            }
        ],
        "ports": [
            {
                "external": "3000",
                "internal": "3000",
                "desc": "Series Troxide desktop gui.",
                "optional": false
            },
            {
                "external": "3001",
                "internal": "3001",
                "desc": "HTTPS Series Troxide desktop gui.",
                "optional": false
            }
        ],
        "custom": [
            {
                "name": "shm-size",
                "name_compose": "shm_size",
                "value": "1gb",
                "desc": "This is required for the application to run properly.",
                "optional": false
            }
        ]
    }
